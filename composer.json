{
  "name": "nadeesha/laravel-si-localization",
  "description": "Sinhala (si) localization for Laravel using JSON translations.",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "localization", "i18n", "l10n", "sinhala", "si", "translations"],
  "homepage": "https://github.com/Nadeesha95/laravel-sinhala-localization-package",
  "support": {
    "issues": "https://github.com/Nadeesha95/laravel-sinhala-localization-package/issues",
    "source": "https://github.com/Nadeesha95/laravel-sinhala-localization-package"
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Nadeesha\\SiLocalization\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nadeesha\\SiLocalization\\SinhalaLocalizationServiceProvider"
      ]
    }
  }
}
