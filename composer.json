{
  "name": "nadeesha/laravel-si-localization",
  "description": "Sinhala (si) localization for Laravel via JSON translations.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Nadeesha\\SiLocalization\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nadeesha\\SiLocalization\\SinhalaLocalizationServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    }
  }
}
